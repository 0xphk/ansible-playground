[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
	editor = nano
[user]
	name = {{ git_username }}
	email = {{ git_useremail }}
[alias]
	logp = log -p
[remote "origin"]
	url = {{ git_remote }}
	fetch = +refs/heads/*:refs/remotes/origin/*
[push]
	default = simple
[branch "master"]
	remote = origin
	merge = refs/heads/master
